  "license": "MIT",
  "devDependencies": {
    "@types/node": "9.4.6",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "cross-env": "5.1.3",
    "prettier": "1.11.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "styled-components": "3.1.6",
    "ts-loader": "3.5.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.9.0",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.2.2",
    "url-loader": "0.6.2",
    "webpack": "3.11.0"
  }
