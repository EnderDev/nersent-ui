{
  "name": "material-react",
  "version": "2.0.0",
  "description": "Material design framework for React",
  "repository": "Nersent/material-react",
  "scripts": {
    "watch": "npm run build:dev && webpack-dev-server --progress --colors --hot --inline --port 8080",
    "build:dev": "rimraf build && webpack",
    "build:production": "rimraf build && cross-env NODE_ENV=production webpack --progress --define process.env.NODE_ENV=production"
  },
  "author": "Nersent",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "node-sass": "4.7.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "uglifyjs-webpack-plugin": "1.1.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}